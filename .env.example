# ====================================
# Telegram Bot Configuration
# ====================================
# احصل على Bot Token من @BotFather في تليجرام
BOT_TOKEN=your_bot_token_here

# معرف المجموعة المستهدفة (يبدأ بـ -)
# احصل عليه من @userinfobot
TARGET_GROUP_ID=-1002469448517

# ====================================
# Supabase Configuration
# ====================================
# رابط مشروعك في Supabase
SUPABASE_URL=https://your-project.supabase.co

# مفتاح anon/public من Supabase
# اذهب إلى Settings > API
SUPABASE_KEY=your_supabase_anon_key_here

# ====================================
# Server Configuration (اختياري)
# ====================================
# المنفذ الذي سيعمل عليه الخادم (افتراضي: 8080)
PORT=8080
